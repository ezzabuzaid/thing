{
  "singleQuote": true,
  "importOrder": [
    "./startup",
    "./instrument",
    "^node:(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@agent/(.*)$",
    "^[./]"
  ],
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
